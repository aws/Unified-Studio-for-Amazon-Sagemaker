# Example environment configuration for integration tests
# Copy to env-local.env and customize for your account
# Add env-*.env to .gitignore

# AWS Configuration (REQUIRED)
export AWS_ACCOUNT_ID=123456789012  # Your 12-digit AWS account ID
export AWS_DEFAULT_REGION=us-east-1  # Default AWS region
export DEV_DOMAIN_REGION=us-east-2  # DataZone domain region for dev environment
export TEST_DOMAIN_REGION=us-east-2  # DataZone domain region for test environment

# DataZone Configuration (REQUIRED)
export DATAZONE_DOMAIN_NAME=Default_12022025_Domain  # DataZone domain name
export DATAZONE_DOMAIN_ID=  # DataZone domain ID (e.g., dzd_abc123def456) - can be auto-discovered
export DATAZONE_PROJECT_NAME_DEV=dev-marketing  # DataZone project name for dev environment
export DATAZONE_PROJECT_NAME_TEST=test-marketing  # DataZone project name for test environment
export DATAZONE_PROJECT_ID_DEV=  # DataZone project ID for dev - can be auto-discovered
export DATAZONE_PROJECT_ID_TEST=  # DataZone project ID for test - can be auto-discovered

# MLflow Configuration (REQUIRED)
export MLFLOW_TRACKING_SERVER_NAME=smus-integration-mlflow-use2  # MLflow tracking server name

# Optional: Custom service endpoints for testing
# export DATAZONE_ENDPOINT_URL=https://datazone.us-east-2.amazonaws.com
# export AWS_ENDPOINT_URL_DATAZONE=https://datazone.us-east-2.amazonaws.com
# export AIRFLOW_SERVERLESS_ENDPOINT=https://airflow-serverless.us-east-2.api.aws/

# Usage:
# 1. Copy this file: cp tests/env-example.env tests/env-local.env
# 2. Edit env-local.env with your account-specific values
# 3. Source before running tests: source tests/env-local.env
# 4. Run tests: python tests/run_tests.py --type integration
