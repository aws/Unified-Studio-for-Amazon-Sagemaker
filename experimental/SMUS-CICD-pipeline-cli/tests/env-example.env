# Example environment configuration for integration tests
# Copy to env-<account-number>.env and customize for your account
# Add env-*.env to .gitignore

# AWS Configuration
export AWS_ACCOUNT_ID=123456789012
export AWS_REGION=us-east-2
export DEV_DOMAIN_REGION=us-east-2

# MLflow Configuration
export MLFLOW_TRACKING_SERVER_NAME=smus-integration-mlflow-use2

# Optional: Custom service endpoints for testing
# export DATAZONE_ENDPOINT_URL=https://datazone.us-east-2.amazonaws.com
# export AWS_ENDPOINT_URL_DATAZONE=https://datazone.us-east-2.amazonaws.com

# Usage:
# source tests/env-<account-number>.env
# pytest tests/integration/basic_app/
